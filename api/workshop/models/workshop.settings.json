    "name": {
      "type": "string"
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "regStartDate": {
      "type": "datetime"
    },
    "regEndDate": {
      "type": "datetime"
    },
    "regPrice": {
      "type": "decimal"
    },
    "maxRegCount": {
      "type": "integer"
    },
    "currentRegCount": {
      "type": "integer",
      "default": 0
    },
    "contacts": {
      "type": "component",
      "repeatable": true,
      "component": "contact.contact"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
