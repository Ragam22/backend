{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "submissionStartDate": {
      "type": "datetime"
    },
    "submissionEndDate": {
      "type": "datetime"
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images", "files", "videos"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "isTeamEvent": {
      "type": "boolean",
      "required": true
    },
    "regStartDate": {
      "type": "datetime",
      "required": true
    },
    "regEndDate": {
      "type": "datetime",
      "required": true
    },
    "regPrice": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "required": true
    },
    "maxRegCount": {
      "type": "integer",
      "default": 100000,
      "required": true
    },
    "currentRegCount": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "contacts": {
      "type": "component",
      "repeatable": true,
      "component": "contact.contact"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "minTeamSize": {
      "type": "integer"
    },
    "maxTeamSize": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "rules": {
      "type": "text"
    },
    "result": {
      "type": "text"
    },
    "announcements": {
      "type": "text"
    },
    "posterImages": {
      "collection": "file",
      "via": "related",
      "allowedTypes": ["images", "files", "videos"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "metaTitles": {
      "type": "json"
    },
    "commonMetaValues": {
      "type": "json"
    },
    "submissionInfo": {
      "type": "component",
      "repeatable": true,
      "component": "submission-info.submission-info"
    },
    "theme": {
      "type": "enumeration",
      "enum": ["Cyberpunk", "Steampunk", "Apocalypse", "Space Odyssey", "Ragnarok", "TakeOne", "TEDxNITC"]
    },
    "category": {
      "type": "enumeration",
      "enum": ["Sports", "General", "Literary", "Music", "Dance", "Kalolsavam", "Quiz", "Ragnarok", "Fine Arts", "TEDxNITC"]
    },
    "regType": {
      "type": "enumeration",
      "enum": ["ragamReg", "kalolsavReg", "payment"],
      "default": "ragamReg",
      "required": true
    }
  }
}
